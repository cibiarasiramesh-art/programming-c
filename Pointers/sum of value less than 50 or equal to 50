#include<stdio.h>
void add(int n,int arr[]){
    int *p=arr;
    int sum=0;
    for (int i=0;i<n;i++) {
        if (*(p+i)>=50) {
            break;
        }
        sum+=*(p+i);
    }
    printf("%d",sum);
}
int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for (int i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    add(n,arr);
}
