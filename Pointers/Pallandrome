#include<stdio.h>
int main() {
    char ch[100];
    scanf("%s",ch);
    int i=0;
    int count=0;
    while (ch[i]!='\0') {
        count++;
        i++;
    }
    int val=0;
    char *p=ch;

    for (int i=0;i<count/2;i++) {
        if (*(p+i)!=*(p+count-i-1)) {
            val=1;
            break;
        }
    }
    if (val==0) {
        printf("pallandrome");
    }
    else if (val==1) {
        printf("not pallandrome");
    }
    return 0;
}
