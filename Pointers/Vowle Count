#include<stdio.h>
#include<string.h>
void vowles(int len,char ch[]) {
    char *p=ch;
    int count=0;
    for (int i=0;ch[i]!='\0';i++) {
        if (*(p+i)=='a'||*(p+i)=='e' || *(p+i)=='i' || *(p+i)=='o' || *(p+i)=='u' || *(p+i)=='A' || *(p+i)=='E'||*(p+i)=='I'
    || *(p+i)=='O' || *(p+i)=='U' ) {
            count++;
        }
    }
    printf("%d",count);

}
int main() {
    char ch[200];
    fgets(ch,sizeof(ch),stdin);
    int len=strlen(ch);
    vowles(len,ch);
    return 0;
}
