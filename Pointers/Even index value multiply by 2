#include<stdio.h>
void even(int n,int arr[]) {

    int *p=arr;
    for (int i=0;i<n;i++) {
        if (i%2==0) {
            printf("%d ",*(p+i)*=2);
        }
        if (i%2!=0) {
            printf("%d ",*(p+i));
        }
    }


}
int main() {
    int n;
    scanf("%d",&n);
    int arr[n];

    for (int i=0;i<n;i++) {
        printf("enter");
        scanf("%d",&arr[i]);
    }
    even(n,arr);
    return 0;

}
