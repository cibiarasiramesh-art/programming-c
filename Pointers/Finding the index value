#include<stdio.h>
int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    int *p=arr;
    for (int i=0;i<n;i++) {
        scanf("%d",(p+i));
    }
    int val;
    int *v=&val;

    scanf("%d",v);
    int not=0;
    int ind;
    for (int i=0;i<n;i++) {
        if (*(p+i)==*v) {
            not=1;
            ind=i+1;
            break;

        }

    }
    if (not==0) {
        printf("-1");
    }
    else if (not==1) {
        printf("%d",ind);
    }
}
