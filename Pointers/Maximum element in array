#include<stdio.h>
void max(int n,int arr[]) {
    int *p=arr;
    int lar=*p;
    for (int i=0;i<n;i++) {
        if (*(p+i)>lar) {
                lar=*(p+i);
        }
    }
    printf("%d",lar);
}
int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for (int i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    max(n,arr);
    return 0;

}
