#include<stdio.h>
void prime(int n) {
    int c=0;
    if (n>=2) {
        for (int i=1;i<=n;i++) {
            if (n%i==0) {
                c++;

            }

        }
        if (c==2) {
            printf("prime number");
        }
        else {
            printf("not a prime number");
        }
    }
    else {
        printf("not a prime number");
    }
}
int main() {
    int n;
    scanf("%d",&n);
    prime(n);
}
