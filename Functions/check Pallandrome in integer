#include<stdio.h>
void pallan(int n) {
    int org=n;
    int rev=0;
    while (n!=0) {
        int rem=n%10;
        rev=rev*10+rem;
        n=n/10;
    }
    if (org==rev) {
        printf("pallandrome");
    }
    else {
        printf("not pallandrome");
    }
}
int main() {
    int n;
    scanf("%d",&n);
    pallan(n);
}


