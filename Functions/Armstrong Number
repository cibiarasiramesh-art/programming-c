#include<stdio.h>
#include<math.h>
void check(int n) {
    int no=n;
    int org=n;
    int count=0;
    while (n!=0) {
        n=n/10;
        count++;
    }
    int total=0;
    while (org!=0) {
        int rem=org%10;
        int val=pow(rem,count);
        total=total+val;
        org=org/10;
    }
    if (no==total) {
        printf("amstrong number");
    }
    else {
        printf("not amstrong number");
    }


}
int main() {
    int n;
    scanf("%d",&n);
    check(n);
    return 0;
}
