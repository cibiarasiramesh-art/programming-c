#include<stdio.h>
int main() {
    int n;
    scanf("%d",&n);
    int count=0;

    int current=0;
    int maxstrike=0;;
    while (n!=0) {
        int v;
        scanf("%d",&v);

        if (v>70){
            count++;
            current++;

            if (current>maxstrike) {
                maxstrike=current;

            }
        }
        else {
            current=0;
        }


        n--;
    }
    printf("%d\n",count);
    printf("%d",maxstrike);
    return 0;
}
