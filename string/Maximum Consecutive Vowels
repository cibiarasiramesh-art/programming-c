#include<stdio.h>
#include<string.h>
int main() {
    char ch[200];
    scanf("%s",ch);
    char val;
        int count=0;
        for (int j=0;ch[j]!='\0';j++) {
            if (ch[j]=='a'||ch[j]=='e'||ch[j]=='i'||ch[j]=='o'||ch[j]=='u'){
                count++;
            }
        }



    printf("%d",count);


}
