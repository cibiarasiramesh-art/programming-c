#include<stdio.h>
#include<string.h>
int main() {
    char ch[200];
    scanf("%s",ch);
     char val;
    int max=0;
    for (int i=0;ch[i]!='\0';i++) {
        int count=1;
        for (int j=i+1;ch[j]!='\0';j++) {
            if (ch[i]==ch[j]) {
                count++;
            }
            else {
                break;
            }


        }
        if (count>max) {
            max=count;
            val=ch[i];
        }


    }
    printf("%c",val);
}
