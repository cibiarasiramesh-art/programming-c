#include<stdio.h>
#include<string.h>
int main() {
    char ch[200];
    fgets(ch,sizeof(ch),stdin);
    int len=strlen(ch);
    int count=0;
    for (int i=0;i<len;i++) {
       if (!(ch[i]>='a' && ch[i]<='z') && !(ch[i]>='A' && ch[i]<='Z') && !(ch[i]>='0' && ch[i]<='9')&& !(ch[i]=='\n')){
           count++;
       }
    }

    printf("%d",count);

}

