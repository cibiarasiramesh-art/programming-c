#include<stdio.h>
int main() {
    int n;
    scanf("%d",&n);

    int arr[n];
    for (int i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    int *s=arr;
    int *e=arr+n-1;
    int temp;
   while (s<e) {
       temp=*s;
       *s=*e;
       *e=temp;
       s++;
       e--;



   }
    for (int i=0;i<n;i++) {
        printf("%d",arr[i]);
    }
    return 0;
}

