#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    for (int i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    int max1, max2, min1, min2;

    if (arr[0] > arr[1]) {
        max1 = arr[0]; max2 = arr[1];
        min1 = arr[1]; min2 = arr[0];
    } else {
        max1 = arr[1]; max2 = arr[0];
        min1 = arr[0]; min2 = arr[1];
    }
    for (int i=2;i<n;i++) {
        if (arr[i]>max1) {
            max2=max1;
            max1=arr[i];
        }
        else if (arr[i]>max2 && arr[i]!=max1){
            max2=arr[i];
        }
    }
    for (int i=2;i<n;i++) {
        if (arr[i]<min1) {
            min2=min1;
            min1=arr[i];
        }
        else if (arr[i]<min2 && arr[i]!=min1) {
            min2=arr[i];
        }
    }
    int maxp=max1*max2;
    int minp=min1*min2;
    if (minp>=maxp) {
        printf("%d",minp);
    }
    else if (maxp>=minp) {
        printf("%d",maxp);
    }
}
