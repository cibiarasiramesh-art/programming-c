#include<stdio.h>
#include<limits.h>
int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for (int i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    int sum=0;
    for (int i=0;i<n;i++) {
        sum=sum+arr[i];
    }
    int avg=sum/n;
    for (int i=0;i<n;i++) {
        if (arr[i]<avg) {
            printf("%d ",arr[i]);
        }

    }

}
