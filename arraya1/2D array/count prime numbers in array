#include<stdio.h>
int main() {
    int r,c;
    scanf("%d %d",&r,&c);
    int arr[r][c];
    for (int i=0;i<r;i++) {
        for (int j=0;j<c;j++) {
            scanf("%d",&arr[i][j]);
        }
    }
    int count=0;
    for (int i=0;i<r;i++) {

        for (int j=0;j<c;j++) {
            int isprime=1;
            if (arr[i][j]>2) {
                for (int k=2;k<arr[i][j];k++) {
                    if (arr[i][j]%k==0) {
                        isprime=0;
                        break;
                    }
                }
            }
            else {
                isprime==0;
                break;
            }

            if (isprime==1){
                count++;
            }
        }

    }
    printf("count: %d",count);
}

}
