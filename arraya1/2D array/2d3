#include<stdio.h>
int main() {
    int r,c;
    scanf("%d %d",&r,&c);
    int arr[r][c];
    for (int i=0;i<r;i++) {
        for (int j=0;j<c;j++) {
            scanf("%d",&arr[i][j]);
        }
    }
    int maxunique=0;
    int row=0;
    for (int i=0;i<r;i++) {
        int unique=0;
        for (int j=0;j<c;j++) {
            int lsUnique=1;
            for (int k=j+1;k<c;k++) {
                if (arr[i][j]==arr[i][k]) {
                    lsUnique=0;
                    break;
                }

            }
            if (lsUnique) {
                unique++;
            }

        }

        if (unique>maxunique){
            maxunique=unique;
            row=i;
        }
    }
    printf("%d",row);


}
