#include<stdio.h>
#include<limits.h>
int main() {
    int r,c;
    scanf("%d %d",&r,&c);
    int arr[r][c];
    for (int i=0;i<r;i++) {
        for (int j=0;j<c;j++) {
            scanf("%d",&arr[i][j]);
        }
    }
    int found=0;
    int count=0;
    int j;
    for (int i=0;i<r;i++) {
        int oddcount=0;
        int evencount=0;
        for (j=0;j<c;j++) {
            if (arr[i][j]%2==0) {
                evencount++;
            }
            else if (arr[i][j]%2!=0) {
                oddcount++;
            }
        }

        if (oddcount==evencount) {
            found=1;
        }

    }
    if (found==1) {
        count++;
    }
    printf("%d",count);
}
