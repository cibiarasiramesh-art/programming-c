#include<stdio.h>
#include<limits.h>
int main() {
    int r,c;
    printf("row and column\n");
    scanf("%d %d",&r,&c);
    int arr[r][c];
    printf("values of row and column\n");
    for (int i=0;i<r;i++) {
        for (int j=0;j<c;j++) {
            scanf("%d",&arr[i][j]);
        }
    }
    int max1=INT_MIN;
    int max2=INT_MIN;
    for (int i=0;i<r;i++) {

        for (int j=0;j<c;j++) {
            if (arr[i][j]>max1) {
                max2=max1;
                max1=arr[i][j];

            }
            else if (arr[i][j]>max2 && arr[i][j]!=max1) {
                max2=arr[i][j];
            }
        }

    }printf("highest salary: %d\n",max1);
    printf("second highest: %d\n",max2);

}
